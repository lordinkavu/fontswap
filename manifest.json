{
  "manifest_version": 3,
  "name": "Fontswap - Change the font of any website",
  "version": "1.0",
  "description": "Change the font of any website to your favorite Google Font",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
